{
    "name": "mickaelbassette/nova-geolocation-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel/nova": "^4.22",
        "spatie/geocoder": "^3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Gabelbart\\Laravel\\Nova\\Fields\\Geolocation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gabelbart\\Laravel\\Nova\\Fields\\Geolocation\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "spatie/geocoder:^3": "Required for geocoding"
    }
}
